<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml"
      alignment="CENTER"
      maxHeight="-Infinity"
      maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="400.0"
      prefWidth="600.0"
      spacing="20.0"
      xmlns="http://javafx.com/javafx"
      fx:controller="q3continuing.app.scenemanagers.HomeSceneFXMLController">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Subjects">
            <font>
                <Font name="System Bold" size="27.0"/>
            </font>
        </Text>
        <GridPane gridLinesVisible="true" prefHeight="80.0" VBox.vgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <VBox alignment="CENTER" spacing="20.0" GridPane.columnIndex="0" GridPane.halignment="CENTER"
                      GridPane.valignment="CENTER" fx:id="subject1">
                    <children>
                        <ImageView fx:id="icon1" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true"
                                   preserveRatio="true"/>
                        <!-- stroke type actually states to remove exterior things -->
                        <Text fx:id="label1" strokeType="OUTSIDE" strokeWidth="0.0">
                            <font>
                                <Font name="System" size="18.0"/>
                            </font>
                        </Text>
                    </children>
                </VBox>
                <VBox alignment="CENTER" spacing="20.0" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                      GridPane.valignment="CENTER" fx:id="subject2">
                    <children>
                        <ImageView fx:id="icon2" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true"
                                   preserveRatio="true"
                        />
                        <Text fx:id="label2" strokeType="OUTSIDE" strokeWidth="0.0">
                            <font>
                                <Font name="System" size="18.0"/>
                            </font>
                        </Text>
                    </children>
                </VBox>
                <VBox alignment="CENTER" spacing="20.0" GridPane.columnIndex="2" GridPane.halignment="CENTER"
                      GridPane.valignment="CENTER" fx:id="subject3">
                    <children>
                        <ImageView fx:id="icon3" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true"
                                   preserveRatio="true"
                        />
                        <Text fx:id="label3" strokeType="OUTSIDE" strokeWidth="0.0">
                            <font>
                                <Font name="System" size="18.0"/>
                            </font>
                        </Text>
                    </children>
                </VBox>
            </children>
        </GridPane>
        <HBox alignment="CENTER" prefHeight="31.0" prefWidth="240.0" spacing="20.0">
            <children>
                <Button fx:id="prev" mnemonicParsing="false" onAction="#previous" prefWidth="50.0" text="Prev"/>
                <Button fx:id="next" mnemonicParsing="false" onAction="#next" prefWidth="50.0" text="Next"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
    </padding>
</VBox>
